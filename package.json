{
  "name": "ng-webpack-autoload",
  "version": "1.0.0",
  "description": "A boilerplate project for autoloading angular modules and components with webpack",
  "main": "dist/server.bundle.js",
  "engines": {
    "node": ">=5"
  },
  "scripts": {
    "clean": "node_modules/.bin/rimraf dist",
    "dev": "./node_modules/.bin/webpack -w --debug --devtool source-map --output-pathinfo",
    "prestart": "npm run clean",
    "start": "npm run dev",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=prod ./node_modules/.bin/webpack --debug --devtool sourcemap",
    "tdd": "./node_modules/.bin/testem",
    "test": "./node_modules/.bin/testem ci -P 2"
  },
  "author": "Amin Meyghani <meyghania@gmail.com> (http://meyghani.com)",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.5.8"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.8",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.6.0",
    "bardjs": "^0.1.8",
    "chai": "^3.5.0",
    "eslint": "^3.4.0",
    "eslint-config-google": "^0.6.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.5",
    "testem": "^1.11.0",
    "webpack": "^1.12.14"
  }
}
